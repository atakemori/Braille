<layer-list xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="24dp"
    android:height="24dp"
    android:viewportWidth="24"
    android:viewportHeight="24">

  <item
      android:width="16dp"
      android:height="16dp"
      android:left="8dp"
      android:top="4dp">
    <shape android:shape="oval"
        android:innerRadius="7dp"
        android:thickness="1dp">
      <solid android:color="@android:color/white"/>
      <stroke android:color="@android:color/black"
          android:width="1dp"/>
    </shape>
  </item>

  <item
      android:width="16dp"
      android:height="16dp"
      android:left="4dp"
      android:top="4dp">
    <shape android:shape="oval"
        android:innerRadius="7dp"
        android:thickness="1dp">
      <solid android:color="@android:color/white"/>
      <stroke android:color="@android:color/black"
          android:width="1dp"/>
    </shape>
  </item>

  <item
      android:width="16dp"
      android:height="16dp"
      android:left="0dp"
      android:top="4dp">
    <shape android:shape="oval"
      android:innerRadius="7dp"
      android:thickness="1dp">
      <solid android:color="@android:color/black"/>
      <stroke android:color="@android:color/black"
          android:width="1dp"/>
    </shape>
  </item>



</layer-list>
